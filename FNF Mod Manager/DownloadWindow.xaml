<Window x:Class="FNF_Mod_Manager.DownloadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FNF_Mod_Manager"
        mc:Ignorable="d" Background="#202020" ResizeMode="NoResize"
        Title="Download" Height="200" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="0.3*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBlock Name="DownloadText" Grid.Row="0" Grid.ColumnSpan="2" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="#f2f2f2" TextWrapping="Wrap"/>
        <Button Grid.Row="1" Grid.Column="0" Content="Yes" Foreground="#f2f2f2" Background="#494949" Margin="50,10,50,10" Click="Yes_Click"/>
        <Button Grid.Row="1" Grid.Column="1" Content="No" Foreground="#f2f2f2" Background="#494949" Margin="50,10,50,10" Click="No_Click"/>
    </Grid>
</Window>
